{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "Boilerplate for Typescript Node application",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "cli": "ts-node src/cli.ts",
    "dev": "tsc --project tsconfig.json --watch & NODE_ENV=development nodemon dist",
    "prestart": "npm run build",
    "start": "nodemon",
    "test": "jest --detectOpenHandles",
    "test-coverage": "jest --coverage",
    "test-invoke": "ts-node --transpile-only ./src/test-invoke.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build",
    "ts-node": "ts-node"
  },
  "author": "ikenley",
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikenley/auth-service"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.441.0",
    "@aws-sdk/client-ssm": "^3.441.0",
    "@vendia/serverless-express": "^4.10.1",
    "axios": "^1.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "helmet": "^5.0.2",
    "jwt-decode": "^4.0.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.114",
    "@types/cookie-parser": "^1.4.5",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
