{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "Boilerplate for Typescript Node application",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "cli": "ts-node src/cli.ts",
    "dev": "tsc --project tsconfig.json --watch & NODE_ENV=development nodemon dist",
    "prestart": "npm run build",
    "start": "nodemon",
    "test": "jest --detectOpenHandles",
    "test-coverage": "jest --coverage",
    "test-invoke": "ts-node --transpile-only ./src/test-invoke.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build",
    "ts-node": "ts-node"
  },
  "author": "ikenley",
  "license": "SEE LICENSE IN LICENSE.txt",
  "repository": {
    "type": "git",
    "url": "https://github.com/ikenley/auth-service"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.787.0",
    "@aws-sdk/client-ssm": "^3.803.0",
    "@vendia/serverless-express": "^4.12.6",
    "axios": "^1.8.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jwt-decode": "^4.0.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "typeorm": "^0.3.22",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.149",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.8",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.26.0",
    "jest": "^29.3.1",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
